Integer n = 1;
Integer n1 = 0;
Integer n2 = 1;
Integer n3 = 0;
  System.debug(n1);
  System.debug(n2);

while(n <= 18){     
    n3 = n2 + n1; 
    System.debug(n3);
    n1 = n3 + n2;
    System.debug(n1);
    n2 = n1 + n3;
    System.debug(n2);
	    
    n++;
    n++;
    n++;
}
  